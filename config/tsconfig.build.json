{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../build"
  },
  "files": [
    "../src/index.tsx"
  ],
  "include": [
    "../src/**/*.tsx"
  ],
  "exclude": [
    "../src/**/*.test.tsx"
  ],
  "typedocOptions": {
    "mode": "file",
    "target": "ES6",
    "out": "generated-docs",
    "name": "React Realm Context",
    "readme": "none",
    "hideGenerator": true,
    "preserveConstEnums": true,
    "stripInternal": true
  }
}
