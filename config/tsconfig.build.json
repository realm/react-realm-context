{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../build"
  },
  "include": [
    "../src/**/*.tsx"
  ],
  "exclude": [
    "../src/**/*.test.tsx"
  ],
  "typedocOptions": {
    "mode": "file",
    "out": "generated-docs",
    "excludePrivate": true,
    "preserveConstEnums": true,
    "stripInternal": true,
    "hideGenerator": true,
    "readme": "none",
    "includeDeclarations": false,
    "name": "React Realm Context",
    "inlineSourceMap": true
  }
}
